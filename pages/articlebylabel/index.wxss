.home-container {
    width: 100%;
    height: 100%;
    background: #eee;
    position: absolute;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
    padding: 15rpx 0;
    box-sizing: border-box;
}
.has-more{
    color: #18e;
}
.no-more{
    display: flex;
    align-items: center;
    justify-content: center;
    font-weight: 300;
    font-size: 28rpx;
    padding: 20rpx;
    color: #666;
}